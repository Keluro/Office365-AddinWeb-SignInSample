                          <bt:Image size="80" resid="library-icon-80" />
                        </Icon>
                        <Action xsi:type="ExecuteFunction">
                          <FunctionName>navigateLibrary</FunctionName>
                        </Action>
                      </Control>
                       <Control xsi:type="Button" id="group.suggestions">
                        <Label resid="residNavigateSuggestionsLabel" />
