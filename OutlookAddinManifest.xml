                          <bt:Image size="80" resid="uservoice-icon-80" />
                        </Icon>
                        <Action xsi:type="ExecuteFunction">
                          <FunctionName>navigateUservoice</FunctionName>
                        </Action>
                      </Control>
                    </Group>
                    <Label resid="residCustomTabLabel" />
                  </CustomTab>
                </CommandSurface>
            </ExtensionPoint>
          </DesktopFormFactor>
